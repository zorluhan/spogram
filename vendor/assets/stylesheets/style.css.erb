.lblfilter{font-size:13px;font-weight:400;color:#999;}
.proposalearn{font-size:23px;font-weight:400;}
.proposalbox{padding:0;margin-bottom:20px;}
.proposalbox a{color:#666;display:block;padding:15px;background:#fff;border:1px solid #eee;border-radius:3px;}
.proposalbox a:hover{text-decoration:none;background:#f9f9f9;border-color:#e7e7e7;}
.proposalname{color:#333;font-weight:400;font-size:16px;margin-bottom:10px;}
.proposalstatus{font-size:12px;}
.proposalstatus i{font-size:13px;}
.proposalstatus.pending{color:#8a6d3b;padding:1px 7px 2px 6px;border-radius:3px;background-color: #fcf8e3;border:1px solid #faebcc;}
.proposalstatus.active{color:#3c763d;padding:1px 7px 2px 6px;border-radius:3px;background-color: #dff0d8;border:1px solid #d6e9c6;}
.proposalstatus.completed{color:#31708f;padding:1px 7px 2px 6px;border-radius:3px;background-color: #d9edf7;border:1px solid #bce8f1;}
.proposalstatus.declined{color:#a94442;padding:1px 7px 2px 6px;border-radius:3px;background-color: #f2dede;border:1px solid #ebccd1;}
.proposaldate{color:#999;font-size:10px;margin-top:10px;}
.proposalinfo{color:#666;font-size:13px;margin-top:15px;background:#fff;border-radius:3px;border:1px solid #eee;padding:15px;}
.proposalinfo1{margin-bottom:20px;}
.btnprop{margin-bottom:5px;background:#E74C3C !important;color:#fff;border-radius:0;}
.btnprop i{font-size:13px;}
.btnprop:hover{color:#fff;opacity:0.9;}
