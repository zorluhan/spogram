.proposalinfo2 span{color:#333;}
.form-propbottom{margin-top:25px;margin-bottom:15px;}
.form-propbottom .gender{color:#666;}
.subtitle3{margin-top:15px;margin-bottom:5px;color: #333;font-size:26px;font-weight:300;}
.proposalfrom{font-size:15px;color:#666;display:block;padding-left:2px;line-height:17px;}
.proptitle{margin-top:35px !important;margin-bottom:25px !important;}
